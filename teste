CREATE TABLE Solicitacao
(
    CodSolicitacao integer PRIMARY KEY AUTOINCREMENT varchar(6),
    nomeSolicitante varchar(30),
    nomeCliente varchar(30),
    grupoEmpresa varchar(50),
    itemUm varchar(30),
    obsItemUm varchar(100),
    itemDois varchar(30),
    obsItemUm
    itemTrez varchar(30),
    obsItemUm
    itemQuatro varchar(30),
    obsItemUm
    itemCinco varchar(30),
    obsItemUm
    itemSeis varchar(30),
    obsItemUm
    itemSete varchar(30),
    obsItemUm
    itemOito varchar(30),
    obsItemUm
    itemNove varchar(30),
    obsItemUm
    itemDez varchar(30),
    obsItemUm
    itemOnze varchar(30),
    obsItemUm
    itemDoze varchar(30),
    obsItemUm
    itemTreze varchar(30),
    obsItemUm
    itemQuatorze varchar(30),
    obsItemUm
    itemQuinz varchar(30),
    obsItemUm
    itemDezesseis varchar(30),
    obsItemUm
    itemDezessete varchar(30),
    obsItemUm
    itemDezoito varchar(30),
    obsItemUm
    itemDezenove varchar(30),
    obsItemUm
    itemVinte varchar(30),
    obsItemUm
    itemVinteUm varchar(30),
    obsItemUm
    itemVinteDois varchar(30),
    obsItemUm
    itemVinteTres varchar(30),
    obsItemUm
    itemVinteQuatro varchar(30),
    obsItemUm
    itemVinteCinco varchar(30),
    obsItemUm
    itemVinteSeis varchar(30),
    obsItemUm
    itemVinteSete varchar(30),
    obsItemUm
    itemVinteOito varchar(30),
    obsItemUm
    itemVinteNove varchar(30)
);