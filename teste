CREATE TABLE Solicitacao
(
    CodSolicitacao integer PRIMARY KEY AUTOINCREMENT varchar(255),
    Nome varchar(255),
   
    itemUm varchar(255),
    itemDois varchar(255),
    itemTrez varchar(255),
    itemQuatro varchar(255),
    itemCinco varchar(255),
    itemSeis varchar(255),
    itemSete varchar(255),
    itemOito varchar(255),
    itemNove varchar(255),
    itemDez varchar(255),
    itemOnze varchar(255),
    itemDoze varchar(255),
    itemTreze varchar(255),
    itemQuatorze varchar(255),
    itemQuinz varchar(255),
    itemDezesseis varchar(255),
    itemDezessete varchar(255),
    itemDezoito varchar(255),
    itemDezenove varchar(255),
    itemVinte varchar(255),
    itemVinteUm varchar(255),
    itemVinteDois varchar(255),
    itemVinteTres varchar(255),
    itemVinteQuatro varchar(255),
    itemVinteCinco varchar(255),
    itemVinteSeis varchar(255),
    itemVinteSete varchar(255),
    itemVinteOito varchar(255),
    itemVinteNove varchar(255)
);